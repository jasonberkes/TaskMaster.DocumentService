{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BlobStorage": "UseDevelopmentStorage=true"
  },
  "BlobStorage": {
    "DefaultContainerName": "documents"
  },
  "InboxProcessor": {
    "Enabled": true,
    "InboxContainerName": "inbox",
    "ProcessedContainerName": "processed",
    "FailedContainerName": "failed",
    "PollingIntervalSeconds": 30,
    "BatchSize": 10,
    "DefaultTenantId": 1,
    "DefaultDocumentTypeId": 1,
    "SystemUser": "InboxProcessor"
  },
  "CodeReviewMigration": {
    "Enabled": false,
    "SourceContainerName": "platform-code-reviews",
    "MigratedContainerName": "code-reviews-migrated",
    "FailedMigrationContainerName": "code-reviews-migration-failed",
    "BatchSize": 50,
    "DefaultTenantId": 1,
    "CodeReviewDocumentTypeId": 2,
    "SystemUser": "CodeReviewMigration",
    "SkipDuplicates": true,
    "PullRequestNumberMetadataKey": "PullRequestNumber",
    "MetadataKey": "CodeReviewMetadata"
  },
  "Meilisearch": {
    "Url": "http://localhost:7700",
    "ApiKey": "",
    "IndexName": "documents",
    "BatchSize": 100,
    "SearchTimeoutSeconds": 5
  },
  "Jwt": {
    "SecretKey": "",
    "ValidIssuer": "",
    "ValidAudience": "",
    "RequireHttpsMetadata": true,
    "ValidateLifetime": true,
    "ClockSkewMinutes": 5
  },
  "ApiKey": {
    "HeaderName": "X-API-Key",
    "Keys": {}
  }
}

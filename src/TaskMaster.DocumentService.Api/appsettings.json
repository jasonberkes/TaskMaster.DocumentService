{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BlobStorage": "UseDevelopmentStorage=true"
  },
  "BlobStorage": {
    "DefaultContainerName": "documents"
  },
  "InboxProcessor": {
    "Enabled": true,
    "InboxContainerName": "inbox",
    "ProcessedContainerName": "processed",
    "FailedContainerName": "failed",
    "PollingIntervalSeconds": 30,
    "BatchSize": 10,
    "DefaultTenantId": 1,
    "DefaultDocumentTypeId": 1,
    "SystemUser": "InboxProcessor"
  },
  "CodeReviewMigration": {
    "Enabled": false,
    "SourceContainerName": "platform-code-reviews",
    "MigratedContainerName": "code-reviews-migrated",
    "FailedMigrationContainerName": "code-reviews-migration-failed",
    "BatchSize": 50,
    "DefaultTenantId": 1,
    "CodeReviewDocumentTypeId": 2,
    "SystemUser": "CodeReviewMigration",
    "SkipDuplicates": true,
    "PullRequestNumberMetadataKey": "PullRequestNumber",
    "MetadataKey": "CodeReviewMetadata"
  },
  "Meilisearch": {
    "Url": "http://localhost:7700",
    "ApiKey": "",
    "IndexName": "documents",
    "BatchSize": 100,
    "SearchTimeoutSeconds": 5
  },
  "Jwt": {
    "SecretKey": "",
    "ValidIssuer": "",
    "ValidAudience": "",
    "RequireHttpsMetadata": true,
    "ValidateLifetime": true,
    "ClockSkewMinutes": 5
  },
  "ApiKey": {
    "HeaderName": "X-API-Key",
    "Keys": {}
  },
  "AzureOpenAI": {
    "Endpoint": "",
    "ApiKey": "",
    "ApiVersion": "2024-02-01",
    "EmbeddingDeployment": "text-embedding-3-small",
    "ChatDeployment": "gpt-4o-mini"
  },
  "VectorMemory": {
    "QdrantEndpoint": "http://localhost:6333",
    "QdrantApiKey": "",
    "AzureOpenAIEndpoint": "",
    "AzureOpenAIApiKey": "",
    "EmbeddingDeploymentName": "text-embedding-3-small",
    "EmbeddingDimension": 1536,
    "DefaultCollectionName": "documents",
    "DefaultTopK": 10,
    "DefaultScoreThreshold": 0.7,
    "EnableRetry": true,
    "MaxRetryAttempts": 3,
    "EnableCircuitBreaker": true,
    "EnableMetrics": true,
    "MaxBatchSize": 100,
    "HttpTimeoutSeconds": 30
  },
  "GraphMemory": {
    "ConnectionString": "localhost:6379",
    "Password": "",
    "DefaultGraphName": "documents",
    "DefaultTraversalDepth": 3,
    "DefaultQueryLimit": 100,
    "EnableRetry": true,
    "MaxRetryAttempts": 3,
    "EnableCircuitBreaker": true,
    "EnableMetrics": true,
    "MaxBatchSize": 100,
    "ConnectionTimeoutSeconds": 30,
    "CommandTimeoutSeconds": 30,
    "UseSsl": false,
    "DatabaseIndex": 0
  }
}
